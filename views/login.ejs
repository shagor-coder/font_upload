<!DOCTYPE html>
<html lang="en">
  <%- include("partials/head",{title: "Login | Font Upload"})%>
  <body>
    <header class="py-1"><%- include("partials/navbar") %></header>
    <main class="py-5">
      <div class="container">
        <form class="row" id="loginForm">
          <div class="col-12">
            <h1>Please Enter your login info</h1>
          </div>
          <div class="col-md-8 mx-auto">
            <div class="mb-3">
              <label for="leInputEmail1" class="form-label text-white"
                >Email address</label
              >
              <input
                type="email"
                class="form-control bg-dark text-white"
                id="leInputEmail1"
                aria-describedby="emailHelp"
              />
            </div>
            <div class="mb-3">
              <label for="leInputPassword1" class="form-label text-white"
                >Password</label
              >
              <input
                type="password"
                class="form-control bg-dark text-white"
                id="leInputPassword1"
              />
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
          </div>
        </form>
      </div>
    </main>
    <%- include('partials/footer') %>
  </body>
</html>
